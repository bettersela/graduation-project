<view class="container">
  <form bindsubmit='formSubmit'>
    <picker mode="date"   name='date' value="{{date}}"    start="1900-01-01"    end="2050-12-12"    bindchange="bindDateChange"  >
    <view class="picker">
      当前选择: {{date}}
    </view>
  </picker>

    <button form-type='submit'>提交</button>
  </form>
</view>
